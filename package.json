{
  "name": "elm-webpack-starter",
  "description": "Webpack setup for writing Elm apps",
  "version": "0.6.0",
  "license": "MIT",
  "author": "Peter Morawiec",
  "repository": {
    "type": "git",
    "url": "https://github.com/moarwick/elm-webpack-starter"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base src/",
    "build": "rm -rf dist && webpack && cp -r src/img/ dist/img/ && cp src/favicon.ico dist/favicon.ico",
    "watch": "webpack --watch",
    "deps:check": "david",
    "deps:up": "david update"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "css-loader": "0.23.1",
    "david": "^7.0.2",
    "elm": "0.17.0",
    "elm-hot-loader": "0.3.2",
    "elm-webpack-loader": "3.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.17.0",
    "postcss-loader": "0.9.1",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "0.12.0"
  }
}
